<Window x:Class="_15var.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_15var"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem x:Name="Category" Header="Категории" Click="Category_OnClick"></MenuItem>
            <MenuItem x:Name="Order" Header="Заказы" Click="Order_OnClick"></MenuItem>
            <MenuItem x:Name="OrderDetail" Header="Детали заказов" Click="OrderDetail_OnClick"></MenuItem>
            <MenuItem x:Name="Product" Header="Продукты" Click="Product_OnClick"></MenuItem>
        </Menu>
    </Grid>
</Window>
